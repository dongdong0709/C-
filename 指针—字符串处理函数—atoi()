//#include <stdlib.h>
//int atoi(const char* nptr);
////功能：
////		atoi()会扫描nptr字符串，跳过前面的空格字符，直到遇到数字或正负号才开始做转换，
////													而遇到非数字或字符串结束符('\0')才结束转换，并将结果返回返回值。
////参数：
////		nptr：待转换的字符串
////返回值：
////		成功转换后整数

#include<stdio.h>
int main() {
	char str1[] = "-10";
	int num1 = atoi(str1);
	printf("num1 = %d\n", num1);

	char str2[] = "0.123";
	double num2 = atof(str2);
	printf("num2 = %lf\n", num2);
}
