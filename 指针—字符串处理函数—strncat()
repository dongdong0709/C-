//#include <string.h>
//char* strncat(char* dest, const char* src, size_t n);
////功能：将src字符串前n个字符连接到dest的尾部，‘\0’也会追加过去
////参数：
////		dest：目的字符串首地址
////		src：源字符首地址
////		n：指定需要追加字符串个数
////返回值：
////		成功：返回dest字符串的首地址
////		失败：NULL
#include<stdio.h>
int main() {
	char str[20] = "123";
	char* src = "hello world";
	printf("%s\n", strncat(str, src, 5));
}
