#include <stdio.h>
int main()
{
	signed int a = -1089474374; //定义有符号整型变量a
	printf("%X\n", a); //结果为 BF0FF0BA
	//-1————>FFFF FFFF FFFF FFFF
	// 原码：100......0001
	// 反码：111......1110
	// 补码：111......1111  在计算机系统中，数值一律用补码来存储
	
	//-2————>FFFF FFFF FFFF FFFE————>1111 1111 1111 1111 1111 1111 1111 1111 1111 1111 1111 1111 1111 1111 1111 1110
	//原码：100......0010
	//反码：111......1101
	//补码：111......1110

	// B    F    0    F    F    0    B	  A
	//1011 1111 0000 1111 1111 0000 1011 1010

	unsigned int b = 3236958022; //定义无符号整型变量b
	printf("%X\n", b); //结果为 C0F00F46

	//当无符号数看，就是一个正数的原码
	// C    0    F    0    0    F    4    6
	//1100 0000 1111 0000 0000 1111 0100 0110
	return 0;
}
